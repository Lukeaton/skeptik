<h1>Articles#index</h1>



<% Article.all.each do |article| %>


    <p><%= link_to image_tag(article.image, :class => 'thumb'), article %></p>
    <%= article.title %>
    <p><%= link_to 'Edit', edit_article_path(article) %></p>
    <p><%= link_to 'Show', article_path(article) %></p>


    <br/>

<% end %>
<br/>
